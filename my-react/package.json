{
  "name": "my-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://salmane-dev.github.io/To-Do/my-react",
  "dependencies": {
    "axios": "^0.20.0",
    "cra-template": "1.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "uuid": "^8.3.1",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "start": "npm run build:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/style.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/assets/css/style.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "gh-pages": "^3.1.0",
    "postcss-cli": "^8.1.0",
    "tailwindcss": "^1.8.13"
  }
}
